{{#unless (noItemsFromPeriodes data.id 'quetes')}}
<ol class="item-list">
    
    <div style="margin-top:20px; margin-bottom:10px; font-family: 'Modesto Condensed', 'Palatino Linotype', 'serif'; font-size: 20px; border-width:0px 0px 2px 0px;border-style:solid;border-color:black;">
        <span>{{localize "NEPH5E.quetes"}}</span>
    </div>

    <div style="column-count:3;">

        {{#each (getLevels data.id 'quetes') as |quete|}}
        <li class="item" data-item-id="{{quete.refid}}" data-item-type="quete">

            <div class="item-name edit-quete">
                <span data-item-id="{{quete.refid}}">{{quete.name}}</span>
            </div>

            <label class="item-integer">{{quete.degre}}</label>

            <div class="item-control">
                <a class="item-roll"><i class="fas fa-dice-d20"></i></a>
            </div>

        </li>
        {{/each}}
        

    </div>

</ol>
{{/unless}}