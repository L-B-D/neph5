            {{/if}}
        {{/with}}
    {{/each}}

    {{> "systems/neph5e/templates/actor/parts/denier/header.hbs" axe="Ouvrier"}}

    {{#each data.denier.pratiques as |pratique|}}
        {{#with (getItem pratique.refid)}}
            {{#if (eq data.data.axe "ouvrier")}}
                {{> "systems/neph5e/templates/actor/parts/denier/pratique.hbs" pratique=pratique}}
            {{/if}}
        {{/with}}
    {{/each}}

    {{> "systems/neph5e/templates/actor/parts/denier/header.hbs" axe="Roi"}}

    {{#each data.denier.pratiques as |pratique|}}
        {{#with (getItem pratique.refid)}}
            {{#if (eq data.data.axe "roi")}}
