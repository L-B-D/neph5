<input
    name="data.page.baton"
    type="hidden"
    value="true"
    data-dtype="Boolean"/>

<ol class="item-list">

    {{> "systems/neph5e/templates/actor/parts/baton/header.hbs" }}

    {{#each data.baton.techniques as |technique|}}
